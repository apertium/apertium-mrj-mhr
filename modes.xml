<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <!-- mrj→mhr direction: -->

  <mode name="mrj-mhr" install="yes">
    <pipeline>
      <program name="hfst-proc --weight-classes 1 -w -e">
        <file name="mrj-mhr.automorf.hfst"/>
      </program>
<!--      <program name="apertium-extract-caps"/> -->
<!--      <program name="cg-proc -w">-->
      <program name="cg-proc -w">
        <file name="mrj-mhr.mor.rlx.bin"/>
      </program>
<!--      <program name="cg-proc -n -1 -w">-->
      <program name="cg-proc -n -1 -w">
        <file name="mrj-mhr.syn.rlx.bin"/>
      </program>
      <program name="apertium-pretransfer"/>
<!--      <program name="lt-proc -b">-->
      <program name="lt-proc -b">
        <file name="mrj-mhr.autobil.bin"/>
      </program>
      <program name="lrx-proc -m">
        <file name="mrj-mhr.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-mrj-mhr.mrj-mhr.t1x"/>
        <file name="mrj-mhr.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-mrj-mhr.mrj-mhr.t2x"/>
        <file name="mrj-mhr.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-mrj-mhr.mrj-mhr.t3x"/>
        <file name="mrj-mhr.t3x.bin"/>
      </program>
<!--      <program name="hfst-proc $1">-->
      <program name="hfst-proc $1">
        <file name="mrj-mhr.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

